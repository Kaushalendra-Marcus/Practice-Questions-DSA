#include<iostream>
using namespace std;
int binarySearch(int arr[],int size,int key){
    int start = 0;
    int end = size-1;
    int mid= start + (end-start)/2; // to prevent stack overflow of memory 
    while(start<=end){
        if(arr[mid]==key){
            return mid;
        }
        //checking in right side part
        else if(arr[mid]<key){
            start= mid +1;
        }
        else{
            end = mid-1;
        }
        mid = (start + end)/2;
    }
    return -1; // element not found
}
int main(){
    int even[7] = {2,9,12,32,69,89,123};
    int index=binarySearch(even,7,123 );
    cout<<"Elment is find out at index of : "<<index<<endl;
    return 0;
}
